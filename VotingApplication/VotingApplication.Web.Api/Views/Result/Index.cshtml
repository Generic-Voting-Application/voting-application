<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/crossfilter/1.3.7/crossfilter.min.js"></script>
<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/d3/3.4.11/d3.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/insightjs/1.2.0/insight.js"></script>
<link type="text/css" rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/insightjs/1.2.0/insight.min.css">

<div class="jumbotron">
    <h1>Lunchtime Voting</h1>
</div>
<div class="row">
    <div class="col-xs-4"></div>
    <div class="col-xs-4 centered">
        <table class="table">
            <tr>
                <th>Option</th>
                <th>Total Votes</th>
                <th>Voters</th>
            </tr>
            <tbody data-bind="foreach: votes">
                <tr>
                    <td data-bind="text: Name"></td>
                    <td data-bind="text: Count"></td>
                    <td data-bind="text: Voters"></td>
                </tr>
            </tbody>
        </table>

        <div id="bar-chart"></div>
    </div>
    <div class="col-xs-4"></div>
</div>